CREATE TABLE `button` (
  `id` int(255) NOT NULL AUTO_INCREMENT,
  `button` varchar(255) NOT NULL,
  `name` varchar(255) NOT NULL,
  `connect` varchar(255) NOT NULL,
  `show` varchar(255) NOT NULL,
  PRIMARY KEY (`id`)
);

