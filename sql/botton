('RegisterButton_Homepage', 'Register', 'Register.php', 'on'),
('LoginButton_Homepage', 'Login', 'Login.php', 'on'),
('ContactButton_Homepage', 'Contact', 'Page7.php', 'on');
('AboutButton_Homepage', 'About', 'Page2.php', 'on');
